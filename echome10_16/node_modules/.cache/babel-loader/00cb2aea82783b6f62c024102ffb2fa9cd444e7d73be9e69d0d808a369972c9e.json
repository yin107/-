{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport breadCrumb from \"components/breadCrumb\";\nexport default {\n  components: {\n    breadCrumb\n  },\n  data() {\n    return {\n      file: null,\n      errorData: [],\n      rightData: [],\n      step: 1,\n      chunkList: []\n    };\n  },\n  methods: {\n    parseFile(file) {\n      this.file = file;\n      console.log(file);\n      this.chunkList = this.createChunk(this.file);\n      console.log(this.chunkList);\n      return false;\n    },\n    //  上传文件、给后端进行解析\n    upFileParse() {\n      if (!this.file) {\n        this.notifyError(\"请选择文件\");\n      } else {\n        this.$request({\n          url: \"/building/parse\",\n          method: \"post\",\n          data: this.file\n        }).then(res => {\n          let res1 = res.data;\n          this.errorData = res1.data.errorData;\n          this.rightData = res1.data.rightData;\n          this.step++;\n          this.file = null;\n        });\n      }\n    },\n    createChunk(file, size = 2 * 1024 * 1024) {\n      const chunkList = [];\n      let cur = 0;\n      while (cur < file.size) {\n        chunkList.push({\n          file: file.slice(cur, cur + size)\n        });\n        cur += size;\n      }\n      return chunkList;\n    }\n  }\n};","map":{"version":3,"names":["breadCrumb","components","data","file","errorData","rightData","step","chunkList","methods","parseFile","console","log","createChunk","upFileParse","notifyError","$request","url","method","then","res","res1","size","cur","push","slice"],"sources":["src/views/basic/bulding/create.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <bread-crumb></bread-crumb>\r\n\r\n    <el-card style=\"margin-top: 20px\">\r\n      <!-- 步骤 -->\r\n      <el-steps :active=\"step\" align-center>\r\n        <el-step title=\"上传数据\" description=\"上传房产数据\"></el-step>\r\n        <el-step title=\"数据验证\" description=\"验证数据是否正确\"></el-step>\r\n        <el-step title=\"导入成功\" description=\"房产数据导入成功\"></el-step>\r\n      </el-steps>\r\n      <!-- 上传 -->\r\n      <el-upload\r\n        class=\"upload-demo\"\r\n        drag\r\n        action=\"/\"\r\n        accept=\"application/vnd.openxmlformats-officedocument.spreadsheetml.sheet\"\r\n        :before-upload=\"parseFile\"\r\n      >\r\n        <i class=\"el-icon-upload\"></i>\r\n\r\n        <div class=\"el-upload-text\">\r\n          <p v-if=\"file\">已选择：{{ file.name }}</p>\r\n          点击或拖拽访固定资产导入文件到此处，导入住在、商户数量受许可限制\r\n        </div>\r\n      </el-upload>\r\n      <el-button type=\"primary\" @click=\"upFileParse\">下一步</el-button>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport breadCrumb from \"components/breadCrumb\";\r\nexport default {\r\n  components: { breadCrumb },\r\n  data() {\r\n    return {\r\n      file: null,\r\n      errorData: [],\r\n      rightData: [],\r\n      step: 1,\r\n      chunkList: [],\r\n    };\r\n  },\r\n  methods: {\r\n    parseFile(file) {\r\n      this.file = file;\r\n      console.log(file);\r\n      this.chunkList = this.createChunk(this.file);\r\n      console.log(this.chunkList);\r\n      return false;\r\n    },\r\n    //  上传文件、给后端进行解析\r\n    upFileParse() {\r\n      if (!this.file) {\r\n        this.notifyError(\"请选择文件\");\r\n      } else {\r\n        this.$request({\r\n          url: \"/building/parse\",\r\n          method: \"post\",\r\n          data: this.file,\r\n        }).then((res) => {\r\n          let res1 = res.data;\r\n          this.errorData = res1.data.errorData;\r\n          this.rightData = res1.data.rightData;\r\n          this.step++;\r\n          this.file = null;\r\n        });\r\n      }\r\n    },\r\n    createChunk(file, size = 2 * 1024 * 1024) {\r\n      const chunkList = [];\r\n      let cur = 0;\r\n      while (cur < file.size) {\r\n        chunkList.push({ file: file.slice(cur, cur + size) });\r\n        cur += size;\r\n      }\r\n      return chunkList;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.upload-demo {\r\n