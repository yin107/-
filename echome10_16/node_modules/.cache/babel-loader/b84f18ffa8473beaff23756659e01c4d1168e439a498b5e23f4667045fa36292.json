n        { prop: \"action\", label: \"操作\" },\r\n      ],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getData();\r\n  },\r\n  methods: {\r\n    // 获取数据\r\n    getData() {\r\n      this.$request({\r\n        url: \"/building/list\",\r\n        method: \"post\",\r\n        data: {\r\n          community_id: 1,\r\n          page_num: 1,\r\n          page_size: 1,\r\n          type: 1,\r\n        },\r\n      }).then((res) => {\r\n        this.tableData = res.data.data.list;\r\n      });\r\n    },\r\n    //builduingType\r\n    buildingType(item) {\r\n      let res = {};\r\n      switch (item) {\r\n        case 1:\r\n          res = { value: \"住宅\", color: \"red\" };\r\n          break;\r\n        case 2:\r\n          res = { value: \"车位\", color: \"blue\" };\r\n          break;\r\n        case 3:\r\n          res = { value: \"仓房\", color: \"pink\" };\r\n          break;\r\n        case 4:\r\n          res = { value: \"商户\", color: \"skyblue\" };\r\n          break;\r\n        case 5:\r\n          res = { value: \"车库\", color: \"orange\" };\r\n          break;\r\n      }\r\n      return res;\r\n    },\r\n\t//detail\r\ndetailInfo(item){\r\n\tlet res=((item.area?item.area:'')+' '+(item.building?item.building:'')+' '+(item.unit?item.unit:'')+' '+item.number+(item.type?(this.buildingType(item.type)):''))\r\n\treturn res\r\n}\r\n  },\r\n  computed: {},\r\n};\r\n</script>\r\n\r\n<style></style>\r\n"],"mappings":"AAqCA,OAAAA,UAAA;AACA;EACAC,IAAA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;IACA;MACAC,SAAA;MACAC,WAAA,GACA;QAAAC,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA,GACA;QAAAD,IAAA;QAAAC,KAAA;MAAA;IAEA;EACA;EACAC,QAAA;IACA,KAAAC,OAAA;EACA;EACAC,OAAA;IACA;IACAD,QAAA;MACA,KAAAE,QAAA;QACAC,GAAA;QACAC,MAAA;QACAV,IAAA;UACAW,YAAA;UACAC,QAAA;UACAC,SAAA;UACAC,IAAA;QACA;MACA,GAAAC,IAAA,CAAAC,GAAA;QACA,KAAAf,SAAA,GAAAe,GAAA,CAAAhB,IAAA,CAAAA,IAAA,CAAAiB,IAAA;MACA;IACA;IACA;IACAC,aAAAC,IAAA;MACA,IAAAH,GAAA;MACA,QAAAG,IAAA;