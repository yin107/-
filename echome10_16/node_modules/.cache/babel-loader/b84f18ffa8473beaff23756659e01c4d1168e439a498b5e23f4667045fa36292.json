{"ast":null,"code":"import breadCrumb from \"components/breadCrumb\";\nexport default {\n  name: \"ListVue\",\n  components: {\n    breadCrumb\n  },\n  data() {\n    return {\n      tableData: [],\n      tableHeader: [{\n        prop: \"id\",\n        label: \"编号\"\n      }, {\n        prop: \"detail\",\n        label: \"详细\"\n      }, {\n        prop: \"type\",\n        label: \"房产类型\"\n      }, {\n        prop: \"created_at\",\n        label: \"导入时间\"\n      }, {\n        prop: \"action\",\n        label: \"操作\"\n      }]\n    };\n  },\n  mounted() {\n    this.getData();\n  },\n  methods: {\n    // 获取数据\n    getData() {\n      this.$request({\n        url: \"/building/list\",\n        method: \"post\",\n        data: {\n          community_id: 1,\n          page_num: 1,\n          page_size: 1,\n          type: 1\n        }\n      }).then(res => {\n        this.tableData = res.data.data.list;\n      });\n    },\n    //builduingType\n    buildingType(item) {\n      let res = {};\n      switch (item) {\n        case 1:\n          res = {\n            value: \"住宅\",\n            color: \"red\"\n          };\n          break;\n        case 2:\n          res = {\n            value: \"车位\",\n            color: \"blue\"\n          };\n          break;\n        case 3:\n          res = {\n            value: \"仓房\",\n            color: \"pink\"\n          };\n          break;\n        case 4:\n          res = {\n            value: \"商户\",\n            color: \"skyblue\"\n          };\n          break;\n        case 5:\n          res = {\n            value: \"车库\",\n            color: \"orange\"\n          };\n          break;\n      }\n      return res;\n    },\n    //detail\n    detailInfo(item) {\n      let res = (item.area ? item.area : '') + ' ' + (item.building ? item.building : '') + ' ' + (item.unit ? item.unit : '') + ' ' + item.number + (item.type ? this.buildingType(item.type) : '');\n      return res;\n    }\n  },\n  computed: {}\n};","map":{"version":3,"names":