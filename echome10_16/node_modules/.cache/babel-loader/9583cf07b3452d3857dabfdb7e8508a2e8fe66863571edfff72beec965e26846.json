rm-item>\r\n      </el-form>\r\n    </el-card>\r\n    <div class=\"btn\">\r\n      <el-button type=\"primary\" @click=\"login()\"> 登录 </el-button>\r\n      <el-button @click=\"$router.push({ name: 'init' })\">注册</el-button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: \"loginVue\",\r\n  data() {\r\n    return {\r\n      loginInfo: {\r\n        account: \"\",\r\n        password: \"\",\r\n        captcha: \"\",\r\n      },\r\n      loginRules: {\r\n        account: [\r\n          { required: true, message: \"请输入账号\" },\r\n          { min: 4, message: \"账号长度应该大于4个字符\" },\r\n        ],\r\n        password: [{ required: true, message: \"请输入密码\" }],\r\n        captcha: [{ required: true, message: \"请输入验证码\" }],\r\n      },\r\n      testUrl: \"\",\r\n    };\r\n  },\r\n  created() {\r\n    //获取验证码图片\r\n    this.$request({\r\n      url: \"/user/captcha\",\r\n      method: \"get\",\r\n    }).then((res) => {\r\n      this.testUrl = res.data.data.img;\r\n    }).catch(err=>{console.log(err);})\r\n  },\r\n  methods: {\r\n    login() {\r\n      //首先进行表单规则的校验\r\n      this.$refs.loginInfo.validate((valid) => {\r\n        if (!valid) return;\r\n        //通过校验 发送请求进行账号密码的验证，如果验证失败的话，就清空表单，并说明失败信息，以及重新刷新验证码\r\n        this.$mylocalrequest({\r\n          url: \"/user/account_login\",\r\n          method: \"post\",\r\n          data: this.loginInfo,\r\n        }).then((res) => {\r\n          //验证成功 表单清空+路由跳转\r\n            this.$store.commit(\"setToken\", res.data.token);\r\n            this.$store.commit(\"setUserInfo\", res.data.userInfo);\r\n            this.$store.commit(\"setPostInfo\", res.data.postInfo);\r\n            \r\n            //登录成功之后跳转页面\r\n\t\t\tif(this.$route.query.redirect==undefined){\r\n\t\t\t\tthis.$router.push({path:'/basic'});\r\n\t\t\t}else {\r\n\t\t\t\tthis.$router.push(this.$route.query.redirect);\r\n\t\t\t}\r\n\r\n\t\t\tthis.loginInfo = {\r\n              account: \"\",\r\n              password: \"\",\r\n              captcha: \"\",\r\n            };\r\n          \r\n        });\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n  .bgcI{\r\n\tposition:absolute;\r\n\tz-index:0\r\n  }\r\n.login {\r\n  width: 600px;\r\n  height: 400px;\r\n  background-color: #fff;\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%);\r\nz-index:1\r\n}\r\n.box-card {\r\n  width: 70%;\r\n  margin: 0 auto;\r\n}\r\n.header {\r\n  height: 40px;\r\n  font-size: 25px;\r\n  border-bottom: 1px solid grey;\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n.btn {\r\n  display: flex;\r\n  justify-content: space-around;\r\n  margin-top: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAkDA;EACAA,IAAA;EACAC,KAAA;IACA;MACAC,SAAA;QACAC,OAAA;QACAC,QAAA;QACAC,OAAA;MACA;MACAC,UAAA;QACAH,OAAA,GACA;UAAAI,QAAA;UAAAC,OAAA;QAAA,GACA;UAAAC,GAAA;UAAAD,OAAA;QAAA,EACA;QACAJ,QAAA;UAAAG,QAAA;UAAAC,OAAA;QAAA;QACAH,OAAA;UAAAE,QAAA;UAAAC,OAAA;QAAA;MACA;MACAE,OAAA;IACA;EACA;EACAC,QAAA;IACA;IACA,KAAAC,QAAA;MACAC,GAAA;MACAC,MAAA;IACA,GAAAC,IAAA,CAAAC,GAAA;MACA,KAAAN,OAAA,GAAAM,GAAA,CAAAf,IAAA,CAAAA,IAAA,CAAAgB,GAAA;IACA,GAAAC,KAAA,CAAAC,GAAA;MAAAC,OAAA,CAAAC,GAAA,CAAAF,GAAA;IAAA;EACA;EACAG,OAAA;IACAC,MAAA;MACA;MACA,KAAAC,KAAA,CAAAtB,SAAA,CAAAuB,QAAA,CAAAC,KAAA;QACA,KAAAA,KAAA;QACA;QACA,KAAAC,eAAA;UACAd,GAAA;UACAC,MAAA;UACAb,IAAA,OAAAC;QACA,GAAAa,IAAA,CAAAC,GAAA;UACA;UACA,KAAAY,MAAA,CAAAC,MAAA,aAAAb,GAAA,CAAAf,IAAA,CAAA6B,KAAA;UACA,KAAAF,MAAA,CAAAC,MAAA,gBAAAb,GAAA,CAAAf,IAAA,CAAA8B,QAAA;UACA,KAAAH,MAAA,CAAAC,MAAA,gBAAAb,GAAA,CAAAf,IAAA,CAAA+B,QAAA;;UAEA;UACA,SAAAC,MAAA,CAAAC,KAAA,CAAAC,QAAA,IAAAC,SAAA;YACA,KAAAC,OAAA,CAAAC,IAAA;cAAAC,IAAA;YAAA;UACA;YACA,KAAAF,OAAA,CAAAC,IAAA,MAAAL,MAAA,CAAAC,KAAA,CAAAC,QAAA;UACA;UAEA,KAAAjC,SAAA;YACAC,OAAA;YACAC,QAAA;YACAC,OAAA;UACA;QAEA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}