{"ast":null,"code":"import { basicRouter } from '@/views/basic/basicRouter';\nimport { iotRouter } from '@/views/iot/iotRouter';\nexport default {\n  name: \"navAside\",\n  mounted() {\n    console.log(this.$route.name);\n  },\n  data() {\n    return {\n      total: [basicRouter, iotRouter]\n    };\n  },\n  computed: {\n    routes() {\n      let res1 = this.$store.state.currentType;\n      let res2 = this.total.filter(item => {\n        return item.path == res1;\n      });\n      return res2[0].children;\n    }\n  },\n  methods: {\n    hasChildrenRoute() {\n      //有子路由\n      return this.routes.filter(item => {\n        return item.children;\n      });\n    },\n    noChidrenRoute() {\n      //没有子路由\n      return this.routes.filter(item => {\n        return !item.children;\n      });\n    }\n  }\n};","map":{"version":3,"names":["basicRouter","iotRouter","name","mounted","console","log","$route","data","total","computed","routes","res1","$store","state","currentType","res2","filter","item","path","children","methods","hasChildrenRoute","noChidrenRoute"],"sources":["src/views/app/components/nav.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h5>物业管理</h5>\r\n    <!-- 导航栏区域 -->\r\n    <el-menu :default-active=\"''\" :router=\"true\">\r\n      <!-- 没有子路由 -->\r\n      <div v-for=\"item of noChidrenRoute()\" :key=\"item.meta.title\">\r\n        <el-menu-item\r\n          :index=\"item.path\"\r\n          v-if=\"item.meta.nav\"\r\n          :class=\"[item.path == $route.path ? 'bgclolr' : '']\"\r\n        >\r\n          {{ item.meta.title }}\r\n        </el-menu-item>\r\n      </div>\r\n\r\n      <!-- 有子路由 -->\r\n      <el-submenu\r\n        v-for=\"item of hasChildrenRoute()\"\r\n        :key=\"item.meta.title\"\r\n        :index=\"item.path\"\r\n      >\r\n        <template slot=\"title\">\r\n          <span>{{ item.meta.title }}</span>\r\n        </template>\r\n        <div v-for=\"chilItem of item.children\" v-bind:key=\"chilItem.meta.title\">\r\n          <el-menu-item\r\n            :index=\"chilItem.path\"\r\n            v-if=\"chilItem.meta.nav\"\r\n            :class=\"[chilItem.path == $route.path ? 'bgclolr' : '']\"\r\n            >{{ chilItem.meta.title }}</el-menu-item\r\n          >\r\n        </div>\r\n      </el-submenu>\r\n    </el-menu>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { basicRouter } from '@/views/basic/basicRouter';\r\nimport { iotRouter } from '@/views/iot/iotRouter';\r\nexport default {\r\n  name: \"navAside\",\r\n  mounted() {\r\n\tconsole.log(this.$route.name);\r\n  },\r\n  data() {\r\n    return {\r\n      total:[basicRouter,iotRouter]\r\n    };\r\n  },\r\n  computed: {\r\n    routes() {\r\n      let res1 = this.$store.state.currentType;\r\n      let res2 = this.total.filter((item) => {\r\n        return item.path == res1;\r\n      });\r\n      return res2[0].children;\r\n    },\r\n  },\r\n  methods: {\r\n    hasChildrenRoute() {\r\n      //有子路由\r\n      return this.routes.filter((item) => {\r\n        return item.children;\r\n      });\r\n    },\r\n    noChidrenRoute() {\r\n      //没有子路由\r\n      return this.routes.filter((item) => {\r\n        return !item.children;\r\n      });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scope>\r\n.bgclolr {\r\n  background-color: skyblue !important;\r\n}\r\n</style>\r\n"],"mappings":"AAuCA,SAAAA,WAAA;AACA,SAAAC,SAAA;AACA;EACAC,IAAA;EACAC,QAAA;IACAC,OAAA,CAAAC,GAAA,MAAAC,MAAA,CAAAJ,IAAA;EACA;EACAK,KAAA;IACA;MACAC,KAAA,GAAAR,WAAA,EAAAC,SAAA;IACA;EACA;EACAQ,QAAA;IACAC,OAAA;MACA,IAAAC,IAAA,QAAAC,MAAA,CAAAC,KAAA,CAAAC,WAAA;MACA,IAAAC,IAAA,QAAAP,KAAA,CAAAQ,MAAA,CAAAC,IAAA;QACA,OAAAA,IAAA,CAAAC,IAAA,IAAAP,IAAA;MACA;MACA,OAAAI,IAAA,IAAAI,QAAA;IACA;EACA;EACAC,OAAA;IACAC,iBAAA;MACA;MACA,YAAAX,MAAA,CAAAM,MAAA,CAAAC,IAAA;QACA,OAAAA,IAAA,CAAAE,QAAA;MACA;IACA;IACAG,eAAA;MACA;MACA,YAAAZ,MAAA,CAAAM,MAAA,CAAAC,IAAA;QACA,QAAAA,IAAA,CAAAE,QAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}