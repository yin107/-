{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport breadCrumb from \"components/breadCrumb\";\nimport { reactive } from \"vue\";\nexport default {\n  components: {\n    breadCrumb\n  },\n  data() {\n    return {\n      file: null,\n      errorData: [],\n      rightData: [],\n      step: 1,\n      chunkList: []\n    };\n  },\n  methods: {\n    parseFile(file) {\n      this.file = file;\n      console.log(file);\n      this.chunkList = this.createChunk(this.file);\n      console.log(this.chunkList);\n      return false;\n    },\n    //  上传文件、给后端进行解析\n    async upFileParse() {\n      if (!this.file) {\n        this.notifyError(\"请选择文件\");\n      } else {\n        // 数据处理\n        const uploadList = this.chunkList.map(({\n          file\n        }, index) => ({\n          file,\n          size: file.size,\n          percent: 0,\n          chunkName: `${this.file.name}-${index}`,\n          fileName: this.file.name,\n          index\n        }));\n        const requestList = uploadList.map(({\n          file,\n          fileName,\n          index,\n          chunkName\n        }) => {\n          const formData = new FormData();\n          formData.append(\"file\", file);\n          formData.append(\"fileName\", fileName);\n          formData.append(\"chunkName\", chunkName);\n          return {\n            formData,\n            index\n          };\n        }).map(({\n          formData\n        }) => {\n          return new Promise(resolve => {\n            //并发请求\n            this.$request({\n              url: \"/building/chunk\",\n              method: \"post\",\n              data: formData\n            }).then(res => {\n              resolve(res);\n            });\n          });\n        });\n        await Promise.all(requestList).then(res => {\n          //如果res是一个数组，如果每一项都是200，就说明每一个分片都上传成功\n          console.log(res);\n          let istrue = res.every(item => item.data.code === 200);\n          console.log(istrue);\n          // 所有的切片都上传成功之后\n        });\n\n        console.log(this.rightData, this.errorData);\n      }\n    },\n    //文件切片\n    createChunk(file, size = 10 * 1024) {\n      const chunkList = [];\n      let cur = 0;\n      while (cur < file.size) {\n        chunkList.push({\n          file: file.slice(cur, cur + size)\n        });\n        cur += size;\n      }\n      return chunkList;\n    }\n  }\n};","map":{"version":3,"names":["breadCrumb","reactive","components","data","file","errorData","rightData","step","chunkList","methods","parseFile","console","log","createChunk","upFileParse","notifyError","uploadList","map","index","size","percent","chunkName","name","fileName","requestList","formData","FormData","append","Promise","resolve","$request","url","method","then","res","all","istrue","every","item","code","cur","push","slice"],"sources":["src/views/basic/bulding/create.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <bread-crumb></bread-crumb>\r\n\r\n    <el-card style=\"margin-top: 20px\">\r\n      <!-- 步骤 -->\r\n      <el-steps :active=\"step\" align-center>\r\n        <el-step title=\"上传数据\" description=\"上传房产数据\"></el-step>\r\n        <el-step title=\"数据验证\" description=\"验证数据是否正确\"></el-step>\r\n        <el-step title=\"导入成功\" description=\"房产数据导入成功\"></el-step>\r\n      </el-steps>\r\n      <!-- 上传 -->\r\n      <el-upload\r\n        class=\"upload-demo\"\r\n        drag\r\n        action=\"/\"\r\n        accept=\"application/vnd.openxmlformats-officedocument.spreadsheetml.sheet\"\r\n        :before-upload=\"parseFile\"\r\n      >\r\n        <i class=\"el-icon-upload\"></i>\r\n\r\n        <div class=\"el-upload-text\">\r\n          <p v-if=\"file\">已选择：{{ file.name }}</p>\r\n          点击或拖拽访固定资产导入文件到此处，导入住在、商户数量受许可限制\r\n        </div>\r\n      </el-upload>\r\n      <el-button type=\"primary\" @click=\"upFileParse\">下一步</el-button>\r\n    </el-card>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport breadCrumb from \"components/breadCrumb\";\r\nimport { reactive } from \"vue\";\r\nexport default {\r\n  components: { breadCrumb },\r\n  data() {\r\n    return {\r\n      file: null,\r\n      errorData: [],\r\n      rightData: [],\r\n      step: 1,\r\n      chunkList: [],\r\n    };\r\n  },\r\n  methods: {\r\n    parseFile(file) {\r\n      this.file = file;\r\n      console.log(file);\r\n      this.chunkList = this.createChunk(this.file);\r\n      console.log(this.chunkList);\r\n      return false;\r\n    },\r\n    //  上传文件、给后端进行解析\r\n    async upFileParse() {\r\n      if (!this.file) {\r\n        this.notifyError(\"请选择文件\");\r\n      } else {\r\n        // 数据处理\r\n        const uploadList = this.chunkList.map(({ file }, index) => ({\r\n          file,\r\n          size: file.size,\r\n          percent: 0,\r\n          chunkName: `${this.file.name}-${index}`,\r\n          fileName: this.file.name,\r\n          index,\r\n        }));\r\n        const requestList = uploadList\r\n          .map(({ file, fileName, index, chunkName }) => {\r\n            const formData = new FormData();\r\n            formData.append(\"file\", file);\r\n            formData.append(\"fileName\", fileName);\r\n            formData.append(\"chunkName\", chunkName);\r\n            return { formData, index };\r\n          })\r\n          .map(({ formData}) => {\r\n            return new Promise((resolve) => {\r\n              //并发请求\r\n              this.$request({\r\n                url: \"/building/chunk\",\r\n                method: \"post\",\r\n                data: formData,\r\n              }).then((res) => {\r\n                resolve(res)\r\n              });\r\n            });\r\n          });\r\n\r\n        await Promise.all(requestList).then(res=>{\r\n\t\t\t//如果res是一个数组，如果每一项都是200，就说明每一个分片都上传成功\r\n\t\t\tconsole.log(res);\r\n\t\t\tlet istrue=res.every(item=>item.data.code===200)\r\n\t\t\tconsole.log(istrue);\r\n\t\t\t// 所有的切片都上传成功之后\r\n\t\t\t\r\n\t\t})\r\n\t\tconsole.log(this.rightData,this.errorData);\r\n      }\r\n    },\r\n    //文件切片\r\n    createChunk(file, size = 10 * 1024) {\r\n      const chunkList = [];\r\n      let cur = 0;\r\n      while (cur < file.size) {\r\n        chunkList.push({ file: file.slice(cur, cur + size) });\r\n        cur += size;\r\n      }\r\n      return chunkList;\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.upload-demo {\r\n  width: 80%;\r\n  margin: 20px auto;\r\n  height: auto;\r\n}\r\n</style>\r\n<style>\r\n.upload-demo .el-upload .el-upload-dragger {\r\n  height: auto;\r\n  width: 600px;\r\n  padding-bottom: 20px;\r\n}\r\n</style>\r\n"],"mappings":";AAgCA,OAAAA,UAAA;AACA,SAAAC,QAAA;AACA;EACAC,UAAA;IAAAF;EAAA;EACAG,KAAA;IACA;MACAC,IAAA;MACAC,SAAA;MACAC,SAAA;MACAC,IAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,UAAAN,IAAA;MACA,KAAAA,IAAA,GAAAA,IAAA;MACAO,OAAA,CAAAC,GAAA,CAAAR,IAAA;MACA,KAAAI,SAAA,QAAAK,WAAA,MAAAT,IAAA;MACAO,OAAA,CAAAC,GAAA,MAAAJ,SAAA;MACA;IACA;IACA;IACA,MAAAM,YAAA;MACA,UAAAV,IAAA;QACA,KAAAW,WAAA;MACA;QACA;QACA,MAAAC,UAAA,QAAAR,SAAA,CAAAS,GAAA;UAAAb;QAAA,GAAAc,KAAA;UACAd,IAAA;UACAe,IAAA,EAAAf,IAAA,CAAAe,IAAA;UACAC,OAAA;UACAC,SAAA,UAAAjB,IAAA,CAAAkB,IAAA,IAAAJ,KAAA;UACAK,QAAA,OAAAnB,IAAA,CAAAkB,IAAA;UACAJ;QACA;QACA,MAAAM,WAAA,GAAAR,UAAA,CACAC,GAAA;UAAAb,IAAA;UAAAmB,QAAA;UAAAL,KAAA;UAAAG;QAAA;UACA,MAAAI,QAAA,OAAAC,QAAA;UACAD,QAAA,CAAAE,MAAA,SAAAvB,IAAA;UACAqB,QAAA,CAAAE,MAAA,aAAAJ,QAAA;UACAE,QAAA,CAAAE,MAAA,cAAAN,SAAA;UACA;YAAAI,QAAA;YAAAP;UAAA;QACA,GACAD,GAAA;UAAAQ;QAAA;UACA,WAAAG,OAAA,CAAAC,OAAA;YACA;YACA,KAAAC,QAAA;cACAC,GAAA;cACAC,MAAA;cACA7B,IAAA,EAAAsB;YACA,GAAAQ,IAAA,CAAAC,GAAA;cACAL,OAAA,CAAAK,GAAA;YACA;UACA;QACA;QAEA,MAAAN,OAAA,CAAAO,GAAA,CAAAX,WAAA,EAAAS,IAAA,CAAAC,GAAA;UACA;UACAvB,OAAA,CAAAC,GAAA,CAAAsB,GAAA;UACA,IAAAE,MAAA,GAAAF,GAAA,CAAAG,KAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAnC,IAAA,CAAAoC,IAAA;UACA5B,OAAA,CAAAC,GAAA,CAAAwB,MAAA;UACA;QAEA;;QACAzB,OAAA,CAAAC,GAAA,MAAAN,SAAA,OAAAD,SAAA;MACA;IACA;IACA;IACAQ,YAAAT,IAAA,EAAAe,IAAA;MACA,MAAAX,SAAA;MACA,IAAAgC,GAAA;MACA,OAAAA,GAAA,GAAApC,IAAA,CAAAe,IAAA;QACAX,SAAA,CAAAiC,IAAA;UAAArC,IAAA,EAAAA,IAAA,CAAAsC,